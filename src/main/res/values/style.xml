<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="develop_button_style1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="develop_style_2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_marginStart">20dp</item>
    </style>

    <style name="DefaultDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowMinWidthMajor">50%</item>
        <item name="android:windowMinWidthMinor">360dp</item>
    </style>

    <style name="show_log_style1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginRight">11dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#868686</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="show_log_style2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginBottom">17dp</item>
        <item name="android:layout_marginLeft">11dp</item>
        <item name="android:layout_marginRight">11dp</item>
        <item name="android:layout_marginTop">11dp</item>
    </style>

    <style name="view_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.5px</item>
        <item name="android:background">#F0F0F0</item>
    </style>

    <style name="color_button_100dp" parent="color_button">
        <item name="android:background">@drawable/frame_color_button_100dp</item>
    </style>

    <style name="color_button">
        <item name="android:layout_width">224dp</item>
        <item name="android:layout_height">46dp</item>
        <item name="android:background">@drawable/frame_color_button</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/enable_white_un_grey</item>
    </style>

    <style name="arrow">
        <item name="android:layout_width">12dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:src">@mipmap/frame_base_icon_arrow</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>


    <style name="default_err_layout_style1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="default_err_layout_style2">
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/default_grey</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="default_err_layout_style3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@drawable/default_loading_err_button</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:textColor">@color/default_grey</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="block">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">#F6F6F6</item>
    </style>


    <style name="select_pay_style1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="select_pay_style2">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="select_pay_style3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/default_black</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="MultiDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowMinWidthMajor">50%</item>
        <item name="android:windowMinWidthMinor">360dp</item>
    </style>

    <style name="select_dialog_style1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingLeft">15dp</item>
    </style>

    <style name="select_dialog_style2">
        <item name="android:layout_width">26dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:layout_marginEnd">13dp</item>
        <item name="android:layout_marginRight">13dp</item>
    </style>

    <style name="select_dialog_style3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/default_black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="frame_loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowMinWidthMajor">50%</item>
        <item name="android:windowMinWidthMinor">360dp</item>
    </style>

    <style name="update_dialog_style1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginRight">11dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/frame_update_dialog_cancel</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#868686</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="update_dialog_style2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginBottom">17dp</item>
        <item name="android:layout_marginLeft">11dp</item>
        <item name="android:layout_marginRight">11dp</item>
        <item name="android:layout_marginTop">11dp</item>
    </style>

    <style name="frame_base_tool_bar_style1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="frame_base_tool_bar_style2">
        <item name="android:layout_width">@dimen/title_bar_height</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
        <item name="android:padding">17dp</item>
    </style>

    <style name="frame_base_tool_bar_style3">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/default_black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="frame_base_tool_bar_style4">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="android:layout_marginEnd">3dp</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="FrameDefaultDialogStyle"  parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/dialogColorAccent</item>
        <item name="android:windowMinWidthMinor">360dp</item>
    </style>

</resources>
